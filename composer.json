{
  "name": "shallowred/printempsdesservicespublics.fr",
  "description": "Pantin's chess club's website",
  "type": "project",
  "keywords": [
    "kirby",
    "cms"
  ],
  "homepage": "https://github.com/shallowred/printempsdesservicespublics.fr#readme",
  "authors": [
    {
      "name": "Lucas Poulain (ShallowRed)",
      "email": "lucaspoulain2@gmail.com"
    }
  ],
  "support": {
    "email": "lucaspoulain2@gmail.com",
    "issues": "https://github.com/shallowred/printempsdesservicespublics.fr/issues",
    "source": "https://github.com/shallowred/printempsdesservicespublics.fr"
  },
  "require": {
    "php": ">=8.3.0",
    "afbora/kirby-loader": "=2.2.0",
    "afbora/kirby-minify-html": "^2.1",
    "arnoson/kirby-vite": "^5.4.0",
    "getkirby/cms": "^4.6.0",
    "lukaskleinschmidt/kirby-snippet-controller": "^2.2",
    "shallowred/kirby-html-sitemap": "^0.1.2",
    "shallowred/kirby-images": "^0.1.1",
    "shallowred/kirby-layouts": "^0.1.3",
    "tobimori/kirby-seo": "^1.1.2",
    "imangazaliev/didom": "^2.0.1",
    "shallowred/kirby-shared-config": "^0.0.1",
    "sylvainjule/matomo": "^2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.11.3"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "kirby-cms-path": false,
    "kirby-plugin-path": "storage/plugins",
    "patches": {
      "sylvainjule/matomo": {
        "Add opt-out block": "patches/matomo-opt-out.patch"
      }
    }
  }
}
